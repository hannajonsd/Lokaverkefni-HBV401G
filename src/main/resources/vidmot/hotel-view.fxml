<AnchorPane  prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vidmot.HotelView">
   <children>
      <Label fx:id="fxUser" layoutX="10.0" layoutY="10.0"/>
      <TableView fx:id="rooms" prefWidth="425.0" prefHeight="200.0" layoutX="96.0" layoutY="155.0">
         <columns>
            <TableColumn fx:id="roomNumberColumn" text="Númer" prefWidth="100.0"/>
            <TableColumn fx:id="typeColumn" text="Týpa" prefWidth="100.0"/>
            <TableColumn fx:id="sizeColumn" text="Stærð" prefWidth="100.0"/>
            <TableColumn fx:id="priceColumn" text="Verð" prefWidth="100.0"/>
         </columns>
      </TableView>
      <Label fx:id="hotelname" layoutX="96.0" layoutY="14.0" prefHeight="30.0" prefWidth="148.0" />
      <Label fx:id="fxAbout" layoutX="96.0" layoutY="44.0" prefHeight="67.0" prefWidth="367.0" wrapText="true" />
      <Button layoutX="30.0" layoutY="356.0" mnemonicParsing="false" onAction="#goHome" text="Heim" />
      <Label fx:id="addOns" layoutX="96.0" layoutY="111.0" prefHeight="43.0" prefWidth="433.0" wrapText="true" />
      <Button layoutX="474.0" layoutY="32.0" mnemonicParsing="false" onAction="#getReviews" text="Fá umsagnir" />
      <Button fx:id="fxBook" layoutX="521.0" layoutY="356.0" mnemonicParsing="false" onAction="#book" text="Bóka" />
   </children>
</AnchorPane>
